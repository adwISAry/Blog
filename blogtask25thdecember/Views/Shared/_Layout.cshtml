    <link rel="stylesheet" href="~/assets/blog/css/style.css">
</head>


<body data-spy="scroll" data-target=".navbar" data-offset="150">

    <!--  PRELOADER  -->
    <div class="loader-area">
        <div class='loader'>
            <div class='one'></div>
            <div class='two'></div>
            <div class='three'></div>
        </div>
    </div>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
